{% if game.users is defined %}
    Joueurs {{game.users|length}}/{{game.maxPlayers}}
    {% for player in game.users %}
        {% if app.request.session.get('userName') == player.login %}
            <br />- <strong>{{player.login | upper }}</strong>
        {% else %}
            <br />-{{player.login | upper }}
        {% endif %}
    {% endfor  %}
{% endif %}
